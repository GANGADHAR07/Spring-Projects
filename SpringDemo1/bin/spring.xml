<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.dxc.beans.Student" id="Student">
		<property name="id" value='101' />
		<property name="name" value="gangu" />
		<property name="email" value="gangu@gmail.com" />
		<property name="mobile" value="934565646"></property>
	</bean>
<bean class="com.dxc.person.Person" id="Person">
		<property name="id" value='101' />
		<property name="name" value="gangu" />
		<property name="email" value="gangu@gmail.com" />
		<property name="mobile" value="934565646"></property>
		<property name="address" ref ="Address"/>
	</bean>
	<bean class="com.dxc.app.Address" id="address">
		<property name="id" value='101' />
		<property name="hno" value="2-332" />
		<property name="street" value="abc" />
		<property name="city" value="chennai"/>
	</bean>
</beans>